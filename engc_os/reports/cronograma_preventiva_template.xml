<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- Template do relatÃ³rio de cronograma de preventiva -->
    <!-- Este template chama o template interno do cronograma -->
    <template id="report_cronograma_preventiva_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <!-- Chama o template interno do cronograma -->
                    <t t-call="engc_os.report_cronograma_preventiva_content">
                        <t t-set="docs" t-value="[o]"/>
                    </t>
                </t>
            </t>
        </t>
    </template>
</odoo>
