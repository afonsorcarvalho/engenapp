<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="sequence_os_seq" model="ir.sequence">
        <field name="name">OS sequence</field>
        <field name="code">engc.os_sequence</field>
        <field name="prefix">OS%(month)s%(y)s.</field>

        <field eval="4" name="padding"/>
       
        <field eval="1" name="number_increment"/>
       
    </record>
    <record id="sequence_calibration_seq" model="ir.sequence">
        <field name="name">Calibration sequence</field>
        <field name="code">engc.calibration_sequence</field>
        <field name="use_date_range">true</field>
        
        <field name="prefix">CAL%(month)s%(y)s.</field>
        <field eval="4" name="padding"/>
        <field eval="1" name="number_increment"/>
       
    </record>
    <record id="sequence_calibration_procedure_seq" model="ir.sequence">
        <field name="name">Calibration procedure sequence</field>
        <field name="code">engc.calibration_procedure_sequence</field>
       
        
        <field name="prefix">PM.</field>
        <field eval="4" name="padding"/>
        <field eval="1" name="number_increment"/>
       
    </record>
    <record id="sequence_calibration_measurement_seq" model="ir.sequence">
        <field name="name">Calibration measurement sequence</field>
        <field name="code">engc.calibration_measurement_sequence</field>
        <field name="use_date_range">true</field>
        
        <field name="prefix">M%(y)s%(month)s%(day)s.</field>
        <field eval="4" name="padding"/>
        <field eval="1" name="number_increment"/>
       
    </record>
    <record id="sequence_os_relatorio_seq" model="ir.sequence">
        <field name="name">Relat√≥rio sequence</field>
        <field name="code">engc.os.relatorio_sequence</field>
        <field name="prefix">RAT%(month)s%(y)s.</field>

        <field eval="4" name="padding"/>
       
        <field eval="1" name="number_increment"/>
       
    </record>
    <record id="sequence_service_request_seq" model="ir.sequence">
        <field name="name">Service Resquest sequence</field>
        <field name="code">engc.service_request_sequence</field>
        <field name="prefix">RS%(month)s%(y)s.</field>

        <field eval="4" name="padding"/>
       
        <field eval="1" name="number_increment"/>
       
    </record>
</odoo>